{
  "name": "functions",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "install:shared": "cd ../shared && npm i",
    "build:shared": "rm -r shared ; cd ../shared && tsc --module commonjs --target es2018 --outDir ../functions/shared",
    "prepare": "firebase functions:config:get > .runtimeconfig.json || exit 0",
    "preinstall": "(npm run install:shared && npm run build:shared && node bundle) || exit 0"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/qs": "^6.9.4",
    "@types/xml2js": "^0.4.5",
    "axios": "^0.21.1",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.12.0",
    "qs": "^6.9.4",
    "shared": "file:shared",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "firebase-functions-test": "^0.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "private": true
}